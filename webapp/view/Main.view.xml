<mvc:View controllerName="demo.demo_pantalla_validaciones.controller.Main" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
						<Button
							activeIcon=""
							enabled="true"
							icon="sap-icon://full-screen"
							iconDensityAware="true"
							iconFirst="true"
							text="Full Width"
							textDirection="Inherit"
							type="Default"
							width="auto"
						>
							
						</Button>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>